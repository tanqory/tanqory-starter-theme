{
  "name": "tanqory-starter-theme",
  "version": "1.0.0",
  "description": "Tanqory Starter Theme - A complete e-commerce theme built with Draftbit's architecture pattern",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./themes": {
      "import": "./dist/themes/index.js",
      "types": "./dist/themes/index.d.ts"
    },
    "./blocks": {
      "import": "./dist/components/blocks/index.js",
      "types": "./dist/components/blocks/index.d.ts"
    },
    "./sections": {
      "import": "./dist/components/sections/index.js",
      "types": "./dist/components/sections/index.d.ts"
    },
    "./screens": {
      "import": "./dist/screens/index.js",
      "types": "./dist/screens/index.d.ts"
    },
    "./apis": {
      "import": "./dist/apis/index.js",
      "types": "./dist/apis/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "tsc && vite build",
    "build:lib": "tsc",
    "preview": "vite preview --host 0.0.0.0 --port 5173",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.12.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.23",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "tanqory",
    "theme",
    "e-commerce",
    "react",
    "draftbit"
  ],
  "license": "MIT"
}
